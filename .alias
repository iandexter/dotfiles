    j=${3}
    for s in $(aws_eb_list-instances ${ebenv} ${r} cgg) ; do
        echo -n "${s}" | cut -d: -f1
        echo -n " "
        ssh -t ${j}/$(echo ${s} | cut -d: -f2) -C "ps -Ao comm,rss" 2>/dev/null | awk '/[h]ttpd/ {sum += $NF} END {print sum}' ;
