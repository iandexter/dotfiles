aws_cluster_info() {
    [[ $# -lt 1 ]] && echo "$FUNCNAME cluster-name [aws-region] [profile]" && return 1
    c=${1}
    r=${2:-ap-northeast-1}
    p=${3:-default}
    container_arns=$(aws ecs list-container-instances --cluster ${c} --region ${r} --profile ${p} | jq -r '.containerInstanceArns[]')
