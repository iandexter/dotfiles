aws_ssh() {
    [[ $# -lt 1 ]] && echo "$FUNCNAME jumphost key user ipaddress" && return 1
    LC_ALL=en_US.UTF-8 ssh -t ${1} ssh -o "StrictHostKeyChecking=no" -o "UserKnownHostsFile=/dev/null" -i /home/iandexter/.ssh/${2}.pem ${3}@${4}
}
